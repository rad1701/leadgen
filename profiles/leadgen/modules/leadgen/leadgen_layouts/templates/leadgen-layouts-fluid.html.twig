{#
/**
 * @file
 * Display Suite LeadGen fluid template.
 */
#}
<{{ outer_wrapper }}{{ attributes.addClass(layout_class, 'leadgen-layouts-fluid', 'fluid-layout', 'clearfix') }}>

{{ title_suffix.contextual_links }}

  <{{ ds_header_wrapper }}{{ ds_header_attributes.addClass('group-header', 'fluid-region') }}>
    {{ ds_header }}
  </{{ ds_header_wrapper }}>

  <{{ ds_content_wrapper }}{{ ds_content_attributes.addClass('group-content') }}>
    {{ ds_content }}
  </{{ ds_content_wrapper }}>

  <{{ ds_main_wrapper }}{{ ds_main_attributes.addClass('group-main', 'fluid-region') }}>
    {{ ds_main }}
  </{{ ds_main_wrapper }}>

  <{{ ds_footer_wrapper }}{{ ds_footer_attributes.addClass('group-footer') }}>
    {{ ds_footer }}
  </{{ ds_footer_wrapper }}>


</{{ outer_wrapper }}>
